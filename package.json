{
  "name": "@withjoy/server-core",
  "version": "1.13.0",
  "description": "Joy Server Base and Utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "engineStrict": true,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "gentype:dev": "apollo client:codegen --endpoint=https://bliss-gateway-staging.withjoy.com/graphql --target=typescript --includes=src/**/*.ts --outputFlat=src/graphql/generated.typings.ts",
    "gentype:local": "apollo client:codegen --endpoint=http://localhost:9091/graphql --target=typescript --includes=src/**/*.ts --outputFlat=src/graphql/generated.typings.ts",
    "postversion": "git push && git push --tags",
    "prebuild": "rm -rf ./dist",
    "prepare": "npm shrinkwrap && yarn build",
    "prepublishOnly": "yarn test",
    "test:only": "NODE_ENV=test TERM=dumb  node  node_modules/.bin/jest  -i --no-coverage --no-verbose  --detectOpenHandles --forceExit --config jest.config.js",
    "test": "NODE_ENV=test jest --detectOpenHandles --no-cache --forceExit --config jest.config.js",
    "version": "git add -A  src test  yarn.lock npm-shrinkwrap.json"
  },
  "peerDependencies": {
    "graphql": "14.4.2"
  },
  "dependencies": {
    "@types/cache-manager": "^2.10.1",
    "@types/chroma-js": "^1.4.1",
    "@types/cookie": "^0.3.3",
    "@types/cors": "2.8.4",
    "@types/express": "4.16.1",
    "@types/forwarded": "~0.1.0",
    "@types/graphql": "^14.2.3",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.136",
    "@types/moment-timezone": "^0.5.12",
    "@types/morgan": "^1.7.37",
    "@types/node-fetch": "2.1.6",
    "@withjoy/telemetry": "^0.6.0",
    "apollo-link-http": "1.5.17",
    "apollo-link": "1.2.14",
    "apollo-server-express": "^2.7.2",
    "apollo-server": "^2.7.2",
    "apollo": "^2.16.2",
    "body-parser": "^1.18.3",
    "cache-manager": "^2.10.1",
    "chroma-js": "^2.0.4",
    "cookie": "^0.4.0",
    "cors": "2.8.5",
    "dataloader": "1.4.0",
    "express": "4.16.4",
    "forwarded": "~0.1.2",
    "graphql-tag": "^2.11.0",
    "graphql": "^14.7.0",
    "immer": "^3.1.3",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.13",
    "moment-timezone": "^0.5.26",
    "morgan": "1.9.1",
    "node-fetch": "2.3.0",
    "typeorm": "0.2.13",
    "typescript": "^3.7.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/lolex": "^3.1.1",
    "@types/nock": "^10.0.2",
    "apollo-server-testing": "^2.7.2",
    "jest": "^25.5.1",
    "lolex": "^4.0.1",
    "nock": "^10.0.6",
    "node-mocks-http": "^1.7.3",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "typemoq": "2.1.0"
  }
}
